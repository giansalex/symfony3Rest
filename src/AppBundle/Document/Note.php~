namespace AppBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Note
{
    /**
     * @MongoDB\Id
     * @var string
     */
    protected $id;

    /**
     * @MongoDB\Field(type="string")
     * @var string
     */
    protected $title;

    /**
     * @MongoDB\Field(type="string")
     * @var string
     */
    protected $value;

    /**
     * @MongoDB\Field(type="string")
     * @var string
     */
    protected $color;


}